<?xml version="1.0" encoding="utf-8"?>
<dashboard>
  <label>Overview</label>
  <description/>
  <earliestTime>rt-360d</earliestTime>
  <latestTime>rt</latestTime>
  <row>
    <single>
      <searchString>sourcetype="ss_log" type="gw" | stats values(req_dn) as a | stats count(a)</searchString>
      <option name="classField">None</option>
      <option name="underLabel">Total level 2 apps</option>
      <option name="linkView">search</option>
    </single>

    <single>
      <searchString>sourcetype="ss_log" type="aie" | stats values(appname) as a | stats count(a)</searchString>
      <option name="classField">None</option>
      <option name="underLabel">Total level 3 apps</option>
      <option name="linkView">search</option>
    </single>

    <single>
      <searchString>sourcetype="ss_log" type="aie" | eval accountname=split(login_name,"@") | eval from_user=mvindex(accountname,0) | stats values(from_user) as a | stats count(a)</searchString>
      <option name="classField">None</option>
      <option name="underLabel">Total users</option>
      <option name="linkView">search</option>
    </single>

  </row>
  <row>
	<chart>
      <title>Top 5 apps</title>
      <searchString>sourcetype="ss_log" type="aie" | top limit=5 appname</searchString>
      <option name="charting.axisTitleX.visibility">collapsed</option>
      <option name="charting.axisTitleY.visibility">collapsed</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">bar</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">stacked</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">bottom</option>
      <drilldown>
        <link>
          <![CDATA[
            /app/splunkforstratusee/search?q=sourcetype="ss_log" type="aie" appname = $click.value$
	  ]]>
        </link>
      </drilldown>
    </chart>
  </row>
  <row>
	<chart>
      <title>Top 5 files</title>
      <searchString>sourcetype="ss_log" type="aie" | top limit=5 objs{}.name</searchString>
      <option name="charting.axisTitleX.visibility">collapsed</option>
      <option name="charting.axisTitleY.visibility">collapsed</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">column</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">stacked</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">bottom</option>
      <drilldown>
        <link>
          <![CDATA[
            /app/splunkforstratusee/search?q=sourcetype="ss_log" type="aie" objs{}.name = $click.value$
      ]]>
        </link>
      </drilldown>
    </chart>

	<chart>
      <title>Top 5 users</title>
      <searchString>sourcetype="ss_log" type="aie" | top limit=5 login_name</searchString>
      <option name="charting.axisTitleX.visibility">collapsed</option>
      <option name="charting.axisTitleY.visibility">collapsed</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">stacked</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">bottom</option>
      <drilldown>
        <link>
          <![CDATA[
            /app/splunkforstratusee/search?q=sourcetype="ss_log" type="aie" login_name = $click.value$
      ]]>
        </link>
      </drilldown>
    </chart>

  </row>

  <row>
    <table>
      <title>The file size > 50M</title>
      <searchString>sourcetype="ss_log" type="aie" objs{}.size>52428800 | rename objs{}.name as File login_name as User appname as App activity as Activity | table _time, File, User, App, Activity</searchString>
      <option name="afterLabel">Total Events</option>
      <option name="classField">None</option>
      <option name="dataOverlayMode">none</option>
      <option name="displayRowNumbers">true</option>
      <option name="drilldown">cell</option>
      <option name="wrap">false</option>
      <option name="count">10</option>
    </table>
  </row>

  <row>
    <chart>
      <title>Top 3 apps each day</title>
      <searchString>sourcetype=ss_log type=aie | timechart span=1d useother=false limit=3 count by appname</searchString>
      <option name="charting.axisTitleX.visibility">collapsed</option>
      <option name="charting.axisTitleY.visibility">collapsed</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">bar</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">stacked</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">bottom</option>
      <drilldown>
        <link>
          <![CDATA[
            /app/splunkforstratusee/search?q=sourcetype="ss_log" type="aie" appname = $click.name2$
      ]]>
        </link>
      </drilldown>
    </chart>


    <chart>
      <title>The 3 unsubscribe apps usage evey day</title>
      <searchString>sourcetype=ss_log type=aie appname!=Box | timechart useother=false sep=1d limit=3 count(appname) by appname</searchString>
      <option name="charting.axisTitleX.visibility">collapsed</option>
      <option name="charting.axisTitleY.visibility">collapsed</option>
      <option name="charting.axisX.scale">linear</option>
      <option name="charting.axisY.scale">linear</option>
      <option name="charting.chart">line</option>
      <option name="charting.chart.nullValueMode">gaps</option>
      <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
      <option name="charting.chart.stackMode">stacked</option>
      <option name="charting.chart.style">shiny</option>
      <option name="charting.drilldown">all</option>
      <option name="charting.layout.splitSeries">0</option>
      <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
      <option name="charting.legend.placement">bottom</option>
      <drilldown>
        <link>
          <![CDATA[
            /app/splunkforstratusee/search?q=sourcetype="ss_log" type="aie" appname = $click.name2$&earliest=$earliest$@h&latest=$latest$
      ]]>
        </link>
      </drilldown>
    </chart>
  </row>

  <row>
    <map>
      <title>Applications by Source IP Location</title>
      <searchString>sourcetype="ss_log" type="gw" | geostats longfield=src_geo.longitude latfield=src_geo.latitude count by req_dn</searchString>
    </map>
  </row>

</dashboard>
